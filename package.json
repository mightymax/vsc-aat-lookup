{
  "name": "aat-lookup",
  "icon": "static/getty_logo_og.png",
  "author": {
    "name": "Mark Lindeman",
    "email": "mark.lindeman@triply.cc"
  },
  "license": "EUPL-1.2",
  "displayName": "aat-lookup",
  "description": "Performs a text query against the AAT data hosted on TriplyDB and returns the identifier.",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.80.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mightymax/vsc-aat-lookup"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "aat-lookup.lookup",
        "title": "AAT Term Lookup"
      }
    ],
    "configuration": [
      {
        "id": "aatLookupSettings",
        "title": "Settings AAT Term Lookup",
        "order": 2,
        "properties": {
          "conf.settingsEditor.aatLookupSettingsApi": {
            "type": "string",
            "order": 2,
            "properties": {
              "searchApi": {
                "type": "string",
                "description": "ElasticSearch Endpoint"
              }
            },
            "default": "https://triplydb.com/_api/datasets/getty/aat/services/aat-text-search/elasticsearch"
          },
          "conf.settingsEditor.aatLookupSettingsOutput": {
            "type": "object",
            "order": 1,
            "properties": {
              "usePrefixer": {
                "type": "boolean",
                "description": "Use TriplyETL vocab syntax"
              },
              "addTermLabel": {
                "type": "boolean",
                "description": "Add the found label as comment"
              }
            },
            "additionalProperties": false,
            "default": {
              "usePrefixer": true,
              "addTermLabel": true
            }
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "axios": "^1.4.0"
  }
}
